  return true;\n        } else {\n          return false;\n        }\n      };\n      const currentChapterIndex = chapterList.findIndex(findChap);\n      // const currentIndex = \"g\";\n      if (currentChapterIndex < 4) {\n        return chapterList[currentChapterIndex + 1].url;\n      }\n      return chapterList[0].url;\n    },\n    currentChapter() {\n      console.log(\"here it is1\");\n      console.log(this.$route.params.chapterNumber);\n      switch (this.$route.params.chapterNumber) {\n        case \"chapter-one\":\n          return chapterOne;\n\n        case \"chapter-two\":\n          return chapterTwo;\n\n        case \"chapter-three\":\n          return chapterThree;\n\n        case \"chapter-four\":\n          return chapterFour;\n\n        case \"chapter-five\":\n          return chapterFive;\n\n        case \"chapter-five\":\n          return chapterFive;\n        case \"chapter-five\":\n          return chapterFive;\n        case \"chapter-five\":\n          return chapterFive;\n\n        default:\n          return (\n            \"<h2 style='color:red;font-family:menlo'>/\" +\n            this.$route.params.chapterNumber +\n            \"</h2>\" +\n            notFound\n          );\n      }\n    },\n  },\n  watch: {},\n};\n</script>"],"sourceRoot":""}]}