er=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"652\">\\n    <tbody>\\n        <tr>\\n            <td width=\"72\" valign=\"top\">\\n                <p align=\"center\">\\n                    11:26\\n                </p>\\n            </td>\\n            <td width=\"75\" valign=\"top\">\\n                <p align=\"center\">\\n                    Terah\\n                </p>\\n            </td>\\n            <td width=\"72\" valign=\"top\">\\n                <p align=\"center\">\\n                    70<strong></strong>\\n                </p>\\n            </td>\\n            <td width=\"434\" valign=\"bottom\">\\n                <p>\\n                    Terah fathered Abram, Nahor and Haran but not triplets,\\nbecause Abram was 70 when Terah died at 205 (11:32, 12:4)                    <strong></strong>\\n                </p>\\n            </td>\\n        </tr>\\n        <tr>\\n            <td width=\"72\" valign=\"bottom\">\\n                <p>\\n                    11:32\\n                </p>\\n            </td>\\n            <td width=\"75\" valign=\"bottom\">\\n                <p align=\"center\">\\n                    Terah\\n                </p>\\n            </td>\\n            <td width=\"72\" valign=\"bottom\">\\n                <p align=\"center\">\\n                    205\\n                </p>\\n            </td>\\n            <td width=\"434\" valign=\"bottom\">\\n                <p>\\n                    Terah died in Haran\\n                </p>\\n            </td>\\n        </tr>\\n        <tr>\\n            <td width=\"72\" valign=\"bottom\">\\n                <p>\\n                    12:4\\n                </p>\\n            </td>\\n            <td width=\"75\" valign=\"bottom\">\\n                <p align=\"center\">\\n                    Abram\\n                </p>\\n            </td>\\n            <td width=\"72\" valign=\"bottom\">\\n                <p align=\"center\">\\n                    75\\n                </p>\\n            <