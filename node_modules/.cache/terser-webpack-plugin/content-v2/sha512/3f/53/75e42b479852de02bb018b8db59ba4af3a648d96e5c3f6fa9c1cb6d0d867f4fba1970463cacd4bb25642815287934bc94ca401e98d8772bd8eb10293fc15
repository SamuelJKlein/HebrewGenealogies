lign=\"bottom\">\\n                <p align=\"center\">\\n                    2\\n                </p>\\n            </td>\\n            <td width=\"126\" valign=\"bottom\">\\n                <p align=\"center\">\\n                    106-108\\n                </p>\\n            </td>\\n            <td width=\"142\" valign=\"bottom\">\\n                <p align=\"center\">\\n                    1900-1898\\n                </p>\\n            </td>\\n        </tr>\\n        <tr>\\n            <td width=\"264\" valign=\"bottom\">\\n                <p>\\n                    With Father-Hebron\\n                </p>\\n            </td>\\n            <td width=\"120\" valign=\"bottom\">\\n                <p align=\"center\">\\n                    22\\n                </p>\\n            </td>\\n            <td width=\"126\" valign=\"bottom\">\\n                <p align=\"center\">\\n                    108-130\\n                </p>\\n            </td>\\n            <td width=\"142\" valign=\"bottom\">\\n                <p align=\"center\">\\n                    1898-1876\\n                </p>\\n            </td>\\n        </tr>\\n        <tr>\\n            <td width=\"264\" valign=\"bottom\">\\n                <p>\\n                    With Joseph-Egypt\\n                </p>\\n            </td>\\n            <td width=\"120\" valign=\"bottom\">\\n                <p align=\"center\">\\n                    17\\n                </p>\\n            </td>\\n            <td width=\"126\" valign=\"bottom\">\\n                <p align=\"center\">\\n                    130-147\\n                </p>\\n            </td>\\n            <td width=\"142\" valign=\"bottom\">\\n                <p align=\"center\">\\n                    1876-1859\\n                </p>\\n            </td>\\n        </tr>\\n    </tbody>\\n</table>\\n<br clear=\"all\"/>\\n<p align=\"center\">\\n    <strong>4.2 Jacob\\'s Chronology-Details</strong>\\n</p>\\n<p>\\n    <strong> Event or Period Genesis Age/Yea