)\\n            </p>\\n        </td>\\n        <td width=\"102\" valign=\"top\">\\n            <p align=\"center\">\\n                1892-32\\n            </p>\\n        </td>\\n        <td width=\"90\" valign=\"top\">\\n            <p align=\"center\">\\n                1860\\n            </p>\\n        </td>\\n        <td width=\"136\" valign=\"top\">\\n            <p>\\n                Exodus 6:18\\n            </p>\\n        </td>\\n    </tr>\\n    <tr>\\n        <td width=\"72\" valign=\"top\">\\n            <p align=\"center\">\\n                4\\n            </p>\\n        </td>\\n        <td width=\"60\" valign=\"top\">\\n            <p align=\"center\">\\n                28\\n            </p>\\n        </td>\\n        <td width=\"192\" valign=\"top\">\\n            <p>\\n                1<sup>st</sup> Missing Generation\\n            </p>\\n        </td>\\n        <td width=\"102\" valign=\"top\">\\n            <p align=\"center\">\\n                1860-28\\n            </p>\\n        </td>\\n        <td width=\"90\" valign=\"top\">\\n            <p align=\"center\">\\n                1832\\n            </p>\\n        </td>\\n        <td width=\"136\" valign=\"top\">\\n        </td>\\n    </tr>\\n    <tr>\\n        <td width=\"72\" valign=\"top\">\\n            <p align=\"center\">\\n                5\\n            </p>\\n        </td>\\n        <td width=\"60\" valign=\"top\">\\n            <p align=\"center\">\\n                28\\n            </p>\\n        </td>\\n        <td width=\"192\" valign=\"top\">\\n            <p>\\n                2<sup>nd</sup> Missing Generation\\n            </p>\\n        </td>\\n        <td width=\"102\" valign=\"top\">\\n            <p align=\"center\">\\n                1832-28\\n            </p>\\n        </td>\\n        <td width=\"90\" valign=\"top\">\\n            <p align=\"center\">\\n                1804\\n            </p>\\n        </td>\\n        <td width=\"136\" valign=\"top\">\\n        </td>\\n    </tr