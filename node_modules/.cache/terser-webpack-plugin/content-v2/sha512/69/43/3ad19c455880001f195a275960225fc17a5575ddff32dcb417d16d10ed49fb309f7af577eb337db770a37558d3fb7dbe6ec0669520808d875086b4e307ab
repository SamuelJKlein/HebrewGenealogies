4\" valign=\"bottom\">\\n            <p>\\n                \"About this time next year\" stated two times\\n            </p>\\n        </td>\\n    </tr>\\n    <tr>\\n        <td width=\"72\" valign=\"top\">\\n            <p>\\n                21:5\\n            </p>\\n        </td>\\n        <td width=\"75\" valign=\"top\">\\n            <p align=\"center\">\\n                Abraham\\n            </p>\\n        </td>\\n        <td width=\"72\" valign=\"top\">\\n            <p align=\"center\">\\n                100\\n            </p>\\n        </td>\\n        <td width=\"434\" valign=\"bottom\">\\n            <p>\\n                Abraham was 100 years old when his son Isaac was born.\\n            </p>\\n        </td>\\n    </tr>\\n    <tr>\\n        <td width=\"72\" valign=\"top\">\\n            <p>\\n                21:8-14\\n            </p>\\n        </td>\\n        <td width=\"75\" valign=\"top\">\\n            <p align=\"center\">\\n                Ishmael\\n            </p>\\n        </td>\\n        <td width=\"72\" valign=\"top\">\\n            <p align=\"center\">\\n                16 (Est)\\n            </p>\\n        </td>\\n        <td width=\"434\" valign=\"bottom\">\\n            <p>\\n                Isaac weaned; Ishmael mocks and is cast out\\n            </p>\\n        </td>\\n    </tr>\\n    <tr>\\n        <td width=\"72\" valign=\"bottom\">\\n            <p>\\n                23:1\\n            </p>\\n        </td>\\n        <td width=\"75\" valign=\"bottom\">\\n            <p align=\"center\">\\n                Sarah\\n            </p>\\n        </td>\\n        <td width=\"72\" valign=\"bottom\">\\n            <p align=\"center\">\\n                127\\n            </p>\\n        </td>\\n        <td width=\"434\" valign=\"top\">\\n            <p>\\n                Sarah died\\n            </p>\\n        </td>\\n    </tr>\\n    <tr>\\n        <td width=\"72\">\\n            <p>\\n                25:7\\n            </p>\\n        </td>\\n      